        }
    ],
    "require": {
	"workerman/workerman":"^3.3.0",
	"topthink/think-installer": "^2.0",
	"topthink/framework":"5.1.*"
    },
    "autoload": {
        "psr-4": {
            "think\\worker\\": "src"
